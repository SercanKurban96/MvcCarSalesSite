@using MvcCarSalesSite.Models
@model IEnumerable<Status>

<ul class="navbar-nav ms-auto mb-2 mb-lg-0">
    @foreach (var item in Model)
    {
        <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/Home/MenuFilter/@item.StatusID">@item.StatusName</a>
        </li>
    }
</ul>
